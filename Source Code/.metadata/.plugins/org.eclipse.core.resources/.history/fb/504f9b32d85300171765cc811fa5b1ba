package com.reyzerbit.guis;

import javax.swing.JFrame;

public class ComabtGUI{
	
	//Frame
	public static JFrame combatGUI = new JFrame();
	
	public static void initCombat(){
		
		combatGUI.getContentPane().setLayout(null);
		combatGUI.setVisible(true);
		combatGUI.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		combatGUI.setSize(800, 500);
		combatGUI.setName("Steven Universe Text Based RPG");
		combatGUI.setTitle("Steven Universe Text Based RPG");
		combatGUI.setResizable(false);
		combatGUI.setLocationRelativeTo(null);
		combatGUI.getRootPane().setDefaultButton(enter);
		
	}
	
}
